<?xml version="1.0" encoding="iso-8859-1" ?>
<otrs_config version="1.0" init="Application">
    <ConfigItem Name="Frontend::Module###ExternalURLJump" Required="0" Valid="1">
        <Description Translatable="1">Frontend module registration for the agent interface.</Description>
        <Group>Znuny4OTRS-ExternalURLJump</Group>
        <SubGroup>Frontend::Agent::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Description>Jump to External URL</Description>
                <Title>Jump to External URL</Title>
                <NavBarName>JumpToExternal</NavBarName>
                <NavBar>
                    <Name Translatable="1">Jump to Znuny!</Name>
                    <Link>Action=ExternalURLJump;URL=http://znuny.com/</Link>
                    <LinkOption>target="_blank"</LinkOption>
                    <NavBar>JumptoExternalURL</NavBar>
                    <Description Translatable="1"></Description>
                    <Type>Menu</Type>
                    <Block>ItemArea</Block>
                    <AccessKey>e</AccessKey>
                    <Prio>10000</Prio>
